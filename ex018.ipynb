    "import math\n",
    "\n",
    "x = float( input('Digite um angulo: '))\n",
    "\n",
    "y = math.cos(math.radians(x))\n",
    "z = math.sin(math.radians(x))\n",
    "a = math.tan(math.radians(x))\n",
    "\n",
    "print('Os valor dos angulos são: cosseno: {:.2f}, seno: {:.2f} e tangente: {:.2f}'.format(y,z,a))"
   ]
